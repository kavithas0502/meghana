In [8]:
num = int(input("Enter a number: "))  
  
if num > 1:  
   for i in range(2,num):  
       if (num % i) == 0:  
           print(num,"is not a prime number")  
           print(i,"times",num//i,"is",num)  
           break  
   else:  
       print(num,"is a prime number")  
         
else:  
   print(num,"is not a prime number")
Enter a number: 2
2 is a prime number
In [9]:
def squaresum(n) : 
 
 
    sm = 0
 
    for i in range(1, n+1) : 
 
        sm = sm + (i * i) 
 
      
 
    return sm
 
 
n = 4
 
print(squaresum(n))
30
